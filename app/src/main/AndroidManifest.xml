            android:theme="@style/MyDialogStyle" />
        <activity
            android:name=".InputWords"
            android:label="@string/title_activity_input_words"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ErrorLearing"
            android:label="@string/title_activity_error_learing"
            android:theme="@style/AppTheme.NoActionBar" />
        <!--
             ATTENTION: This was auto-generated to add Google Play services to your project for
             App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".SelectTable"
            android:label="@string/title_activity_select_table"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CreateTableDialog"
            android:theme="@style/MyDialogStyle" />
        <activity
            android:name=".CreateWordsListDialog"
            android:theme="@style/MyDialogStyle" />
        <activity
            android:name=".TestSelectTable"
            android:label="@string/title_activity_test_select_table"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
